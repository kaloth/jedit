# Properties file for the SnoutKick plugin

# jEdit only needs to load the plugin the first time the user accesses it
# the presence of this property also tells jEdit the plugin is using the new API
plugin.SnoutKickPlugin.activate=defer

# Information about the plugin
plugin.SnoutKickPlugin.name=SnoutKick
plugin.SnoutKickPlugin.version=1.0
plugin.SnoutKickPlugin.author=Christopher Plant
plugin.SnoutKickPlugin.description=A simple ctags sidebar for quickly browsing symbols

# help documentation
plugin.SnoutKickPlugin.docs=index.html

# dependencies
plugin.SnoutKickPlugin.depend.0=jedit 05.00.99.00
plugin.SnoutKickPlugin.depend.1=jdk 1.6

# plugin menu
plugin.SnoutKickPlugin.menu=SnoutKick \
	- \
	SnoutKick.refresh

# action labels for actions supplied by dockables.xml
SnoutKick.label=SnoutKick

# action labels for actions supplied by actions.xml
SnoutKick.refresh.label=Refresh View
SnoutKick.next.label=Goto Next Tag
SnoutKick.prev.label=Goto Prev Tag

# plugin option pane
plugin.SnoutKickPlugin.option-pane=SnoutKick

# Option pane activation BeanShell snippet
options.SnoutKick.code=new SnoutKickOptionPane();

# Option pane labels
options.SnoutKick.label=SnoutKick
options.SnoutKick.file=File:
options.SnoutKick.choose-file=Choose
options.SnoutKick.choose-file.title=Where is ctags?
options.SnoutKick.choose-sort.title=Sort by...

# window title
SnoutKick.title=SnoutKick

# Setting not defined but supplied for completeness
options.SnoutKick.ctagspath=
options.SnoutKick.sorttype=0
